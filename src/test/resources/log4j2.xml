<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		 <Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{YYYY-MM-dd HH:mm:ss} [%level] %logger{36} - %msg%n" />
		</Console>
	</Appenders>
	
	<Loggers>
		
		<Root level="WARN">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>
	
</Configuration>